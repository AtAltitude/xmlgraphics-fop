  <property type="generic">
    <name>conditional-length-template</name>
    <class-name>GenericCondLength</class-name>
    <datatype>CondLength</datatype>
    <compound>
      <subproperty set-by-shorthand="true">
        <name>length</name>
        <datatype>Length</datatype>
